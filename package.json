    "flow-bin": "^0.63.1",
    "jest": "^22.0.4",
    "prettier": "^1.9.2"
  },
  "dependencies": {
    "@babel/cli": "^7.0.0-beta.36",
    "@babel/runtime": "^7.0.0-beta.36"
